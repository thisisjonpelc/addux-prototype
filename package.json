  "scripts": {
    "start": "node server/server.js",
    "test": "jest",
    "serve": "live-server public/",
    "build": "webpack --watch",
    "build:prod": "webpack -p --env production",
    "dev-server": "webpack-dev-server",
    "heroku-postbuild": "yarn run build:prod",
    "compile:css": "node-sass src/styles/main.scss public/css/style.css",
    "watch:css": "nodemon -e scss -x \"npm run compile:css\"",
    "compile:sass": "node-sass src/styles/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last10 versions\" public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "yarn run compile:sass prefix:css compres:css"
  },
