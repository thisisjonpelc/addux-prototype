  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.2",
    "bcrypt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "browserslist": "^4.2.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "express-csv": "^0.6.0",
    "html2canvas": "^1.0.0-alpha.12",
    "jquery": "^3.3.1",
    "json2csv": "^4.3.2",
    "jsonwebtoken": "^8.3.0",
    "jspdf": "^1.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongodb": "^3.1.4",
    "mongoose": "^5.2.14",
    "nodemailer": "^4.6.8",
    "react": "^16.5.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "react-stripe-elements": "^2.0.1",
    "redux": "^4.0.0",
    "stripe": "^6.12.1",
    "style-loader": "^0.23.0",
    "throttle-debounce": "^2.0.1",
    "validator": "^10.7.1",
    "webpack": "^4.19.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "serve": "live-server public/",
    "build": "webpack --watch",
    "build:prod": "webpack -p --env production",
    "dev-server": "webpack-dev-server",
    "heroku-postbuild": "yarn run build:prod",
    "compile:css": "node-sass src/styles/main.scss public/css/style.css",
    "watch:css": "nodemon -e scss -x \"npm run compile:css\"",
    "compile:sass": "node-sass src/styles/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last10 versions\" public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "yarn run compile:sass prefix:css compres:css"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "postcss-cli": "^6.0.0",
    "source-map-explorer": "^1.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
